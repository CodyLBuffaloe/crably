<div class="title">
    <h2 id="login">Welcome to Crab.ly!</h2>
</div>


<h1>Login</h1>

<%= form_tag user_sessions_path, method: :post do %>
    <div class="field">

        <%= label_tag :email %>
        <%= text_field_tag :email %>
        <br>

    </div>
    <br>
    <div class="field">

        <%= label_tag :password %>
        <%= password_field_tag :password%>
        <br>

    </div>
    <br>

    <div class="actions">

        <%= submit_tag "Login" %>

    </div>

<% end %>
<div class="nav">
    <div class="nav_box">
        <div class="nav_link">
            <%= link_to "Back", users_path %>
        </div>
    </div>

</div>